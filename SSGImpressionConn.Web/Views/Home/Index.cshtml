            <div>
                <table id="appointment" class="table table-bordered table-hover table-dark">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.AppointmentID)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Name)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.AppointmentDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.AppointmentTime)
                        </th>
                        <th>
                            Barber Name
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.ServiceDetail.ServiceName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.ShopDetail.ShopName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Status.Status1)
                        </th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.ID)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.AppointmentDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.TimeSlot.Time)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.BarberDetail.Name)
                            </td>
                            <td>
                                @foreach (var items in item.AppointServices)
                                {
                                    string val = "";
                                    string s1 = items.ApointServiceIDs.ToString();
                                    string[] s2 = s1.Split(',');
                                    foreach (string s3 in s2)
                                    {
                                        val = s3;
                                        var checkID = int.Parse(val);

                                        foreach (var serv in db.ServiceDetails.ToList())
                                        {
                                            if (serv.ID == checkID)
                                            {
                                                <span>@serv.ServiceName,</span>
                                            }

                                        }
                                    }
                                    @*int id = 0;
                                        var val = items.ApointServiceIDs.ToString();
                                        for (int i = 0; i < val.Length; i++)
                                        {
                                            id = val[i];
                                            foreach (var serv in items.AppointmentDetail.serviceDetails.Where(x => x.ID == id))
                                            {
                                                <span>@serv.ServiceName</span>
                                            }
                                        }
                                        if (items.ApointServiceIDs.Any())
                                        {
                                            <span>@items.AppointmentDetail.ServiceDetail.ServiceName</span>
                                        }*@
                                }
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ShopDetail.ShopName)
                            </td>
                            @if (item.Status != null)
                            {
                                if (item.Status.Status1.ToLower() == "waiting")
                                {
                                    <td style="color:darkgoldenrod">
                                        @Html.DisplayFor(modelItem => item.Status.Status1)
                                    </td>
                                }
                                if (item.Status.Status1.ToLower() == "Done")
                                {
                                    <td style="color:darkred">
                                        @Html.DisplayFor(modelItem => item.Status.Status1)
                                    </td>
                                }
                                if (item.Status.Status1.ToLower() == "Processing")
                                {
                                    <td style="color:green">
                                        @Html.DisplayFor(modelItem => item.Status.Status1)
                                    </td>
                                }
                            }
                            else
                            {
                                <td class="text-success">
                                </td>
                            }

                        </tr>
                    }

                </table>

            </div>
