        background: url(images/much_01.png) center center no-repeat;
        position: relative;
    }

    .section_heading h3 {
        color: #9e8a78;
        font-weight: 400;
    }

    .section_heading h2 {
        font-size: 36px;
        line-height: 48px;
        font-weight: 400;
        letter-spacing: 0;
    }
